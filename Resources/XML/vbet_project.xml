<?xml version="1.0" encoding="utf-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="project_schema.xsd">
  <name>V-BET</name>
  <test>/Name</test>
  <node>
    <label xpath="Name"/>
    <children collapsed="false">
      <node>
        <label>Inputs</label>
        <children>
          <repeater>
            <label>Topography</label>
            <xpath>/inputs/topography/DEM</xpath>
            <node>
              <label xpath="name"></label>
              <type>DEM</type>
              <entity>
                <type>raster</type>
                <xpath>filepath</xpath>
              </entity>
              <children>
                <repeater>
                  <label>Flow Accumulation</label>
                  <xpath>/inputs/topography/DEM/Flows</xpath>
                  <node>
                    <label xpath="name"></label>
                    <type>flow accumulation</type>
                    <entity>
                      <type>raster</type>
                      <xpath>filepath</xpath>
                    </entity>
                  </node>
                </repeater>
                <repeater>
                  <label>Slope</label>
                  <xpath>/inputs/topography/DEM/Slope</xpath>
                  <node>
                    <label xpath="name"></label>
                    <type>slope</type>
                    <entity>
                      <type>raster</type>
                      <xpath>filepath</xpath>
                    </entity>
                  </node>
                </repeater>
              </children>
            </node>
          </repeater>
          <repeater>
            <label>Network</label>
            <xpath>/inputs/drainage_network/network</xpath>
            <node>
              <label xpath="name"></label>
              <type>network</type>
              <entity>
                <type>vector</type>
                <xpath>filepath</xpath>
              </entity>
              <children>
                <repeater>
                  <label>Buffers</label>
                  <xpath>inputs/drainage_network/network/buffers</xpath>
                  <node>
                    <label xpath="name"></label>
                    <type>buffers</type>
                    <entity>
                      <type>vector</type>
                      <xpath>filepath</xpath>
                    </entity>
                  </node>
                </repeater>
              </children>
            </node>
          </repeater>
        </children>
      </node>
      <node>
        <label>Analyses</label>
        <children>
          <repeater>
            <label>Output</label>
            <xpath>/analyses/output</xpath>
            <node>
              <label xpath="name"></label>
              <type>valley bottom</type>
              <entity>
                <type>vector</type>
                <xpath>filepath</xpath>
              </entity>
            </node>
          </repeater>
        </children>
      </node>
    </children>
  </node>
</project>