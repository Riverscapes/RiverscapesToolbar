<?xml version="1.0" encoding="utf-8"?>
<Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="XSD/project_explorer.xsd">
  <Name>VBET</Name>
  <ProjectType>VBET</ProjectType>
  <Node>
    <Label xpath="/Name"/>
    <Children>
      <Repeater>
        <!--This is a template for how each realization should render-->
        <Label>Inputs</Label>
        <Xpath>/Inputs</Xpath>
        <Node>
          <Label xpath="Name"/>
        </Node>
      </Repeater>
      <Repeater>
        <!--This is a template for how each realization should render-->
        <Label>Realizations</Label>
        <Xpath>/Realizations</Xpath>
        <Node>
          <Label xpath="Name"/>
          <Children collapsed="false">
            <Node>
              <Label>Inputs</Label>
            </Node>
            <Node>
              <Label>Analyses</Label>
            </Node>
          </Children>
        </Node>
      </Repeater>
    </Children>
  </Node>
</Project>