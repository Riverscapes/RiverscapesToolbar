<?xml version="1.0" encoding="utf-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="project_schema.xsd">
  <name>Sample</name>
  <test>/sampleName</test>
  <node>
    <label xpath="sampleName"/>
    <children collapsed="false">
      <repeater>
        <label>Inputs</label>
        <xpath>/inputs/input</xpath>
        <node>
          <label xpath="name"/>
          <type>SOME_TYPE</type>
          <entity>
            <type>raster</type>
            <xpath>filepath</xpath>
            <symbology>DEM</symbology>
          </entity>
        </node>
      </repeater>
      <repeater>
        <label>Outputs</label>
        <xpath>/outputs/output</xpath>
        <node>
          <label xpath="name"/>
          <type>SOME_TYPE</type>
          <entity>
            <type>raster</type>
            <xpath>filepath</xpath>
            <symbology>DEM</symbology>
          </entity>
        </node>
      </repeater>
    </children>
  </node>
</project>